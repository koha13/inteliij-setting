<application>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="inheritors" />
        <option value="problems" />
        <option value="references" />
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JAVA" value="false" />
        <entry key="JSON" value="true" />
        <entry key="Markdown" value="false" />
        <entry key="SQL" value="false" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="false" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_INTENTION_PREVIEW" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JavaScript.js.chain.hints" />
        <option value="JavaScript.js.type.hints" />
        <option value="SQL.sql.join.cardinality.hints" />
        <option value="TypeScript.ts.chain.hints" />
        <option value="TypeScript.ts.enum.hints" />
        <option value="TypeScript.ts.type.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="SQL.parameter.hints.old" />
    <settingsMapElement>
      <JavaScript.js.type.hints>
        <Settings>
          <option name="showForInferredParameters" value="false" />
        </Settings>
      </JavaScript.js.type.hints>
      <TypeScript.ts.type.hints>
        <Settings>
          <option name="showForInferredParameters" value="false" />
        </Settings>
      </TypeScript.ts.type.hints>
      <JAVA.annotation.hints>
        <Settings>
          <option name="showInferred" value="true" />
        </Settings>
      </JAVA.annotation.hints>
    </settingsMapElement>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>